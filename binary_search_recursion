#include <iostream>
#include<cstring>
#include<bits/stdc++.h>
using namespace std;
int n,x;
int bsearch(int low,int high,int a[],int x){
    if (high >= low) {
        int mid=(low+(high-1))/2;
    if(a[mid]==x)
        return mid;
    if(a[mid]>x)
        return bsearch(low,mid-1,a,x);
    else
        return bsearch(mid+1,high,a,x);
    }
 
       
    return -1;
}
    
 
int main(){
   cin>>n;
   int a[n];
  for(int i=0;i<n;i++)
    cin>>a[i];
    cin>>x;
    int index=bsearch(0,n-1,a,x);
        if(index)
         cout<<"element found at index "<<index;
       
    else
         cout<<"element not found";
        
        
    
    return 0;
}
